#!/usr/bin/env python
import requests
from bs4 import BeautifulSoup

BASE_URL = 'http://ellinofreneianet.gr'
LIST_SHOWS_URL = BASE_URL + '/radio/radio-shows-2.html'

html_doc = requests.get(LIST_SHOWS_URL).content
soup = BeautifulSoup(html_doc, 'html.parser')
links = []

for link in soup.find_all('a'):
    url = link.get('href')
    if ((url and
         url.startswith('/radio/radio-shows-2/') and
         url.endswith('html'))):
        full_url = BASE_URL + url
        if full_url not in links:
            links.append(full_url)

for link in links[::-1]:
    print link
